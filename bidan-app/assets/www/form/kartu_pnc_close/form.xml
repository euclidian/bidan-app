<?xml version="1.0" encoding="UTF-8"?>
<form autocomplete="off" class="or clearfix" id="PNC_Close" novalidate="novalidate">
    <!--This form was created by transforming a OpenRosa-flavored (X)Form using an XSLT sheet created by Enketo LLC.-->
    <section class="form-logo" />
    <h3 id="form-title">Penutupan PNC</h3>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span class="question-label active" lang="">Alasan menutup PNC</span>
                <span class="required">*</span>
                <span class="or-hint active" lang="">Pilihlah salah satu alasan yang tersedia</span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/PNC_Close/pnc_close_reason" data-name="/PNC_Close/pnc_close_reason"
                        data-type-xml="select1" required="required"
                        type="radio" value="death_of_mother" />
                    <span class="option-label active" lang="">Ibu meninggal</span>
                </label>
                <label class="">
                    <input name="/PNC_Close/pnc_close_reason" data-name="/PNC_Close/pnc_close_reason"
                        data-type-xml="select1" required="required"
                        type="radio" value="permanent_relocation" />
                    <span class="option-label active" lang="">Relokasi</span>
                </label>
                <label class="">
                    <input name="/PNC_Close/pnc_close_reason" data-name="/PNC_Close/pnc_close_reason"
                        data-type-xml="select1" required="required"
                        type="radio" value="end_of_pp_period" />
                    <span class="option-label active" lang="">Akhir periode postpartum</span>
                </label>
                <label class="">
                    <input name="/PNC_Close/pnc_close_reason" data-name="/PNC_Close/pnc_close_reason"
                        data-type-xml="select1" required="required"
                        type="radio" value="wrong_entry" />
                    <span class="option-label active" lang="">Kesalahan entry</span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <section name="/PNC_Close/death_mother_group" class="or-group-data or-branch pre-init "
        data-relevant=" /PNC_Close/pnc_close_reason  = 'death_of_mother'">
        <label class="question non-select ">
            <span class="or-constraint-msg active" lang="">Tanggal kematian ibu hari ini atau
                hari-hari sebelumnya
            </span>
            <span class="question-label active" lang="">Tanggal kematian</span>
            <span class="or-hint active" lang="">masukkan tanggal kematian</span>
            <input name="/PNC_Close/death_mother_group/death_date" data-constraint=".&lt;= /PNC_Close/today "
                data-type-xml="date" type="date" />
        </label>
        <fieldset class="question ">
            <fieldset>
                <legend>
                    <span class="question-label active" lang="">apakah penyebab kematian ibu?
                    </span>
                    <span class="or-hint active" lang="">Pilihlah salah satu jawaban yang tersedia
                    </span>
                </legend>
                <div class="option-wrapper">
                    <label class="">
                        <input name="/PNC_Close/death_mother_group/maternal_death" data-name="/PNC_Close/death_mother_group/maternal_death"
                            data-type-xml="select1" type="radio"
                            value="yes" />
                        <span class="option-label active" lang="">Penyebab langsung</span>
                    </label>
                    <label class="">
                        <input name="/PNC_Close/death_mother_group/maternal_death" data-name="/PNC_Close/death_mother_group/maternal_death"
                            data-type-xml="select1" type="radio"
                            value="no" />
                        <span class="option-label active" lang="">Penyebab tidak langsung</span>
                    </label>
                    <label class="">
                        <input name="/PNC_Close/death_mother_group/maternal_death" data-name="/PNC_Close/death_mother_group/maternal_death"
                            data-type-xml="select1"
                            type="radio" value="dont_know" />
                        <span class="option-label active" lang="">Tidak tahu</span>
                    </label>
                </div>
            </fieldset>
        </fieldset>
        <fieldset class="question or-branch pre-init ">
            <fieldset>
                <legend>
                    <span class="question-label active" lang="">Penyebab Kematian ibu?</span>
                    <span class="or-hint active" lang="">Pilihlah salah satu jawaban yang tersedia
                    </span>
                </legend>
                <div class="option-wrapper">
                    <label class="">
                        <input name="/PNC_Close/death_mother_group/maternal_death_cause"
                            data-name="/PNC_Close/death_mother_group/maternal_death_cause"
                            data-relevant=" /PNC_Close/death_mother_group/maternal_death  = 'yes'"
                            data-type-xml="select1"
                            type="radio"
                            value="prolonged_obstructed_labor" />
                        <span class="option-label active" lang="">Persalinan macet /
                            berkepanjangan
                        </span>
                    </label>
                    <label class="">
                        <input name="/PNC_Close/death_mother_group/maternal_death_cause"
                            data-name="/PNC_Close/death_mother_group/maternal_death_cause"
                            data-relevant=" /PNC_Close/death_mother_group/maternal_death  = 'yes'"
                            data-type-xml="select1"
                            type="radio"
                            value="hypertension_fits" />
                        <span class="option-label active" lang="">Hipertensi parah / tiba-tiba
                        </span>
                    </label>
                    <label class="">
                        <input name="/PNC_Close/death_mother_group/maternal_death_cause"
                            data-name="/PNC_Close/death_mother_group/maternal_death_cause"
                            data-relevant=" /PNC_Close/death_mother_group/maternal_death  = 'yes'"
                            data-type-xml="select1"
                            type="radio"
                            value="bleeding_hemorrhage" />
                        <span class="option-label active" lang="">Pendarahan</span>
                    </label>
                    <label class="">
                        <input name="/PNC_Close/death_mother_group/maternal_death_cause"
                            data-name="/PNC_Close/death_mother_group/maternal_death_cause"
                            data-relevant=" /PNC_Close/death_mother_group/maternal_death  = 'yes'"
                            data-type-xml="select1"
                            type="radio"
                            value="fever_infection" />
                        <span class="option-label active" lang="">Demam tinggi / infeksi</span>
                    </label>
                    <label class="">
                        <input name="/PNC_Close/death_mother_group/maternal_death_cause"
                            data-name="/PNC_Close/death_mother_group/maternal_death_cause"
                            data-relevant=" /PNC_Close/death_mother_group/maternal_death  = 'yes'"
                            data-type-xml="select1"
                            type="radio"
                            value="cause_not_identified" />
                        <span class="option-label active" lang="">Penyebab tidak teridentifikasi
                        </span>
                    </label>
                    <label class="">
                        <input name="/PNC_Close/death_mother_group/maternal_death_cause"
                            data-name="/PNC_Close/death_mother_group/maternal_death_cause"
                            data-relevant=" /PNC_Close/death_mother_group/maternal_death  = 'yes'"
                            data-type-xml="select1"
                            type="radio"
                            value="others" />
                        <span class="option-label active" lang="">Lainnya</span>
                    </label>
                </div>
            </fieldset>
        </fieldset>
        <label class="question or-branch pre-init non-select ">
            <span class="question-label active" lang="">Lainnya</span>
            <span class="or-hint active" lang="">Sebutkan alasan lainnya Jika ada</span>
            <input name="/PNC_Close/death_mother_group/other_maternal_death_cause" data-relevant=" /PNC_Close/death_mother_group/maternal_death_cause  = 'others'"
                data-type-xml="string"
                type="text" />
        </label>
    </section>
    <!--end of group -->
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span class="question-label active" lang="">konfirmasi penutupan PNC?</span>
                <span class="required">*</span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/PNC_Close/confirm_pnc_close" data-name="/PNC_Close/confirm_pnc_close"
                        data-type-xml="select1" required="required" type="radio"
                        value="yes" />
                    <span class="option-label active" lang="">Ya</span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <fieldset style="display:none;" id="or-preload-items">
        <label class="calculation non-select ">
            <input name="/PNC_Close/start" data-preload="timestamp" data-preload-params="start"
                data-type-xml="dateTime" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/PNC_Close/today" data-preload="date" data-preload-params="today"
                data-type-xml="date" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/PNC_Close/deviceid" data-preload="property" data-preload-params="deviceid"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/PNC_Close/simserial" data-preload="property" data-preload-params="simserial"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/PNC_Close/phonenumber" data-preload="property" data-preload-params="phonenumber"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/PNC_Close/end" data-preload="timestamp" data-preload-params="end"
                data-type-xml="dateTime" type="hidden" />
        </label>
    </fieldset>
    <fieldset style="display:none;" id="or-calculated-items">
        <label class="calculation non-select ">
            <input name="/PNC_Close/death_mother_group/is_mother_closed" autocomplete="off"
                data-calculate="if( /PNC_Close/death_mother_group/maternal_death  == 'yes', 'true', 'false')"
                data-type-xml="string"
                type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/PNC_Close/reg_date" autocomplete="off" data-calculate=" /PNC_Close/today "
                data-type-xml="date" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/PNC_Close/is_mother_dead" autocomplete="off" data-calculate="/PNC_Close/death_mother_group/is_mother_closed"
                data-type-xml="string"
                type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/PNC_Close/death_mother_group/woman_survived" data-calculate="&quot;no&quot;"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/PNC_Close/meta/instanceID" data-calculate="concat('uuid:', uuid())"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/PNC_Close/formhub/uuid" data-calculate="'4f124ab3762d4edbbbc9c56dad18a2db'"
                data-type-xml="string" type="hidden" />
        </label>
    </fieldset>
</form>
