         <input type="date" name="/Penutupan_ANC/induced_abortion_group/date_induced" data-constraint=".&lt;= /Penutupan_ANC/today " data-type-xml="date" />
      </label>
      <fieldset class="question or-branch pre-init ">
         <fieldset>
            <legend>
               <span lang="" class="question-label active">apakah diberikan injeksi Rh</span>
            </legend>
            <div class="option-wrapper">
               <label class="">
                  <input type="radio" name="/Penutupan_ANC/induced_abortion_group/rh_injection_given2" data-name="/Penutupan_ANC/induced_abortion_group/rh_injection_given2" value="yes" data-relevant=" /Penutupan_ANC/spontaneous_abortion_group/case_w_rh_negative  != ''" data-type-xml="select1" />
                  <span lang="" class="option-label active">Ya</span>
               </label>
               <label class="">
                  <input type="radio" name="/Penutupan_ANC/induced_abortion_group/rh_injection_given2" data-name="/Penutupan_ANC/induced_abortion_group/rh_injection_given2" value="no" data-relevant=" /Penutupan_ANC/spontaneous_abortion_group/case_w_rh_negative  != ''" data-type-xml="select1" />
                  <span lang="" class="option-label active">Tidak</span>
               </label>
            </div>
         </fieldset>
      </fieldset>
      <fieldset class="question ">
         <fieldset>
            <legend>
               <span lang="" class="question-label active">Berikan konseling tentang metode KB kepada pasangan</span>
            </legend>
            <div class="option-wrapper">
               <label>
                  <input value="OK" type="radio" name="/Penutupan_ANC/induced_abortion_group/induced_fp_message" data-type-xml="string" />
                  <span class="option-label active" lang="">OK</span>
               </label>
            </div>
         </fieldset>
      </fieldset>
   </section>
   <!--end of group -->
   <section class="or-group-data or-branch pre-init " name="/Penutupan_ANC/maternal_death_group" data-relevant=" /Penutupan_ANC/maternal_death  = 'yes'">
      <label class="question non-select ">
         <span lang="" class="or-constraint-msg active">Tanggal kematian ibu hari ini atau hari-hari sebelumnya</span>
         <span lang="" class="question-label active">Tanggal kematian ibu?</span>
         <span class="required">*</span>
         <span lang="" class="or-hint active">Masukkan tanggal kematian Ibu</span>
         <input type="date" name="/Penutupan_ANC/maternal_death_group/date_maternal_death" required="required" data-constraint=".&lt;= /Penutupan_ANC/today " data-type-xml="date" />
      </label>
      <fieldset class="question ">
         <fieldset>
            <legend>
               <span lang="" class="question-label active">Penyebab kematian ibu?</span>
            </legend>
            <div class="option-wrapper">
               <label class="">
                  <input type="radio" name="/Penutupan_ANC/maternal_death_group/maternal_death_cause" data-name="/Penutupan_ANC/maternal_death_group/maternal_death_cause" value="abortion" data-type-xml="select1" />
