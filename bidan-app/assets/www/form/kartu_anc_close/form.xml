   <fieldset id="or-calculated-items" style="display:none;">
   <label class="calculation non-select ">
         <input autocomplete="off" type="hidden" name="/Penutupan_ANC/reg_date" data-calculate=" /Penutupan_ANC/today " data-type-xml="date" />
      </label>
      <label class="calculation non-select ">
         <input type="hidden" name="/Penutupan_ANC/num_abortions" data-relevant=" /Penutupan_ANC/close_reason  = 'induced_abortion' or  /Penutupan_ANC/close_reason  = 'spontaneous_abortion'" data-calculate=" /Penutupan_ANC/case_num_abortions  + 1" data-type-xml="string" />
      </label>
      <label class="calculation non-select ">
         <input type="hidden" name="/Penutupan_ANC/spontaneous_abortion_group/num_abortions_spontaneous" data-calculate=" /Penutupan_ANC/spontaneous_abortion_group/case_num_abortions_spontaneous  + 1" data-type-xml="string" />
      </label>
      <label class="calculation non-select ">
         <input type="hidden" name="/Penutupan_ANC/spontaneous_abortion_group/num_weeks_spontaneous" data-calculate="int(( /Penutupan_ANC/spontaneous_abortion_group/date_spontaneous  -  /Penutupan_ANC/spontaneous_abortion_group/case_lmp ) div 7)" data-type-xml="string" />
      </label>
      <label class="calculation non-select ">
         <input type="hidden" name="/Penutupan_ANC/spontaneous_abortion_group/threemonths_from_spont_abortion_date" data-calculate="date( /Penutupan_ANC/spontaneous_abortion_group/date_spontaneous  + 90)" data-type-xml="string" />
