      </label>
      <label class="calculation non-select ">
         <input type="hidden" name="/Penutupan_ANC/induced_abortion_group/num_abortions_induced" data-calculate=" /Penutupan_ANC/induced_abortion_group/case_num_abortions_induced  + 1" data-type-xml="string" />
      </label>
      <label class="calculation non-select ">
         <input type="hidden" name="/Penutupan_ANC/rh_injection_given" data-calculate="if( /Penutupan_ANC/close_reason  = 'spontaneous_abortion',  /Penutupan_ANC/spontaneous_abortion_group/rh_injection_given1 ,  /Penutupan_ANC/induced_abortion_group/rh_injection_given2 )" data-type-xml="string" />
      </label>
      <label class="calculation non-select ">
         <input type="hidden" name="/Penutupan_ANC/meta/instanceID" data-calculate="concat('uuid:', uuid())" data-type-xml="string" />
      </label>
      <label class="calculation non-select ">
         <input type="hidden" name="/Penutupan_ANC/formhub/uuid" data-calculate="'83821b53dbe2443690dcfa35c9758d05'" data-type-xml="string" />
      </label>
   </fieldset>
