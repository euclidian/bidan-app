   <fieldset class="question or-branch pre-init ">
      <fieldset>
         <legend>
            <span lang="" class="question-label active">Konfirmasi penutupan ANC?</span>
            <span class="required">*</span>
         </legend>
         <div class="option-wrapper">
            <label class="">
               <input type="radio" name="/Penutupan_ANC/confirm_Penutupan_ANC" data-name="/Penutupan_ANC/confirm_Penutupan_ANC" value="yes" required="required" data-relevant=" /Penutupan_ANC/close_reason  != 'relocation_temporary'" data-type-xml="select1" />
               <span lang="" class="option-label active">Ya</span>
            </label>
         </div>
      </fieldset>
   </fieldset>
