               <input type="radio" name="/Penutupan_ANC/close_reason" data-name="/Penutupan_ANC/close_reason" value="relocation_temporary" required="required" data-type-xml="select1" />
               <span lang="" class="option-label active">Relokasi (sementara)</span>
