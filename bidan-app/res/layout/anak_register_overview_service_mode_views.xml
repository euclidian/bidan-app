<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:weightSum="73"
              android:orientation="horizontal">

    <org.ei.bidan.view.customControls.CustomFontTextView
        android:id="@+id/txt_ibu_ki_no"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="10"
        style="@style/CustomFontTextViewStyle.ListView.Medium"/>

    <ImageView
        android:layout_width="@dimen/smart_register_client_divider_height"
        android:layout_height="fill_parent"
        android:src="@color/smart_register_client_divider_color"/>

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="17">

        <org.ei.bidan.view.customControls.CustomFontTextView
            android:id="@+id/lbl_anak_register_dob"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="Tgl Lhr : "
            style="@style/CustomFontTextViewStyle.ListView.Medium"
            android:textStyle="normal"/>

        <org.ei.bidan.view.customControls.CustomFontTextView
            android:id="@+id/anak_register_dob"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@id/lbl_anak_register_dob"
            style="@style/CustomFontTextViewStyle.ListView.Medium.NoPadding.NoBold"/>

        <org.ei.bidan.view.customControls.CustomFontTextView
            android:id="@+id/lbl_tempat_lahir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/lbl_anak_register_dob"
            android:text="Tmpt : "
            style="@style/CustomFontTextViewStyle.ListView.Medium"
            android:textStyle="normal"/>

        <org.ei.bidan.view.customControls.CustomFontTextView
            android:id="@+id/tempat_lahir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/lbl_tempat_lahir"
            android:layout_below="@id/lbl_anak_register_dob"
            android:text="Puskesmas"
            style="@style/CustomFontTextViewStyle.ListView.Medium.NoPadding.NoBold"
            android:textStyle="normal"/>

        <org.ei.bidan.view.customControls.CustomFontTextView
            android:id="@+id/lbl_berat_lahir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/lbl_tempat_lahir"
            android:text="B.Lahir : "
            style="@style/CustomFontTextViewStyle.ListView.Medium"
            android:textStyle="normal"/>

        <org.ei.bidan.view.customControls.CustomFontTextView
            android:id="@+id/berat_lahir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/lbl_berat_lahir"
            android:layout_below="@id/lbl_tempat_lahir"
            android:text="3300"
            style="@style/CustomFontTextViewStyle.ListView.Medium.NoPadding.NoBold"
            android:textStyle="normal"/>

        <org.ei.bidan.view.customControls.CustomFontTextView
            android:id="@+id/lbl_tipe_lahir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/lbl_berat_lahir"
            android:text="Type : "
            style="@style/CustomFontTextViewStyle.ListView.Medium"/>

        <org.ei.bidan.view.customControls.CustomFontTextView
            android:id="@+id/tipe_lahir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/lbl_tipe_lahir"
            android:layout_below="@id/lbl_berat_lahir"
            android:text="Normal"
            style="@style/CustomFontTextViewStyle.ListView.Medium.NoPadding.NoBold"
            android:textStyle="normal"/>

    </RelativeLayout>


    <ImageView
            android:layout_width="@dimen/smart_register_client_divider_height"
            android:layout_height="fill_parent"
            android:src="@color/smart_register_client_divider_color"/>

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="15">

        <ImageView
            android:id="@+id/icon_imd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@drawable/ic_no_small"/>

        <ImageView
            android:id="@+id/icon_imd_yes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@drawable/ic_no"
            android:visibility="gone"/>

        <org.ei.bidan.view.customControls.CustomFontTextView
            android:id="@+id/is_imd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@id/icon_imd"
            style="@style/CustomFontTextViewStyle.ListView.Medium.NoPadding.NoBold"
            android:text="IMD"/>

        <ImageView
            android:id="@+id/icon_immuni_hb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/is_imd"
            android:src="@drawable/ic_yes_small"/>

        <org.ei.bidan.view.customControls.CustomFontTextView
            android:id="@+id/is_immuni_hb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/icon_immuni_hb"
            android:layout_below="@+id/is_imd"
            style="@style/CustomFontTextViewStyle.ListView.Medium.NoPadding.NoBold"
            android:text="Immuni HB"/>

        <ImageView
            android:id="@+id/icon_vit_k"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/is_immuni_hb"
            android:src="@drawable/ic_yes_small"/>

        <org.ei.bidan.view.customControls.CustomFontTextView
            android:id="@+id/is_vit_k"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/icon_vit_k"
            android:layout_below="@+id/is_immuni_hb"
            style="@style/CustomFontTextViewStyle.ListView.Medium.NoPadding.NoBold"
            android:text="Vit K"/>

        <ImageView
            android:id="@+id/icon_salep_mata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/is_vit_k"
            android:src="@drawable/ic_yes_small"/>

        <org.ei.bidan.view.customControls.CustomFontTextView
            android:id="@+id/is_salep_mata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/icon_salep_mata"
            android:layout_below="@+id/is_vit_k"
            style="@style/CustomFontTextViewStyle.ListView.Medium.NoPadding.NoBold"
            android:text="Salep Mata"/>

    </RelativeLayout>

    <ImageView
            android:layout_width="@dimen/smart_register_client_divider_height"
            android:layout_height="fill_parent"
            android:src="@color/smart_register_client_divider_color"/>

    <include
            layout="@layout/anak_register_client_birth_status"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="@integer/child_list_sick_status_view_weight"/>

    <ImageView
            android:layout_width="@dimen/smart_register_client_divider_height"
            android:layout_height="fill_parent"
            android:src="@color/smart_register_client_divider_color"/>

    <ImageButton
            android:id="@+id/btn_edit"
            android:layout_width="@dimen/list_edit_icon_view_width"
            android:layout_height="fill_parent"
            android:layout_weight="@integer/child_list_edit_icon_view_weight"
            android:scaleType="center"
            android:gravity="center"
            android:layout_margin="5dp"
            android:clickable="true"
            android:focusable="true"
            android:background="@drawable/edit_button_background"/>

    <include
        layout="@layout/anak_register_client_last_service"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="@integer/child_list_last_service_view_weight"/>
</LinearLayout>